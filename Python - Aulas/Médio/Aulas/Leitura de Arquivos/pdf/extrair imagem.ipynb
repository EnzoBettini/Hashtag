    "\n",
    "arquivo = Pdf.open('MGLU_ER_3T20_POR.pdf')\n",
    "\n",
    "for pagina in arquivo.pages:\n",
    "    for nome, imagem in pagina.images.items():\n",
    "        imagem_salvar = PdfImage(imagem)\n",
    "        imagem_salvar.extract_to(fileprefix=f'imagens/{nome}')"
   ]
  }
